{
  "name": "application",
  "version": "2.0.0",
  "description": "Hapi api development",
  "license": "MIT",
  "main": "server.js",
  "engines": {
    "node": ">=8.3.0",
    "npm": ">=5.3.0",
    "yarn": ">=0.27.5"
  },
  "scripts": {
    "start": "node index.js",
    "lint": "esw  **/*.js --ignore-pattern node_modules/ config --color",
    "coverage": ". ./env.sh && nyc -r cobertura -r text-summary -r html npm test",
    "lint:watch": "npm run lint -- --watch",
    "test:watch": "npm t -- -w",
    "npm:secure": "nsp check",
    "debug": ". ./env.sh && NODE_ENV=test nodemon --inspect=0.0.0.0:9229 app/server.js"
  },
  "dependencies": {
    "axios": "0.16.2",
    "bluebird": "^3.5.0",
    "boom": "5.2.0",
    "bunyan": "1.8.12",
    "config": "1.26.2",
    "glob": "7.1.2",
    "good": "7.3.0",
    "good-console": "6.4.0",
    "hapi": "16.5.2",
    "hapi-swagger": "7.7.1",
    "http-status": "1.0.1",
    "inert": "4.2.1",
    "joi": "10.6.0",
    "lodash": "4.17.4",
    "mongoose": "^5.3.6",
    "newrelic": "2.2.0",
    "vision": "4.1.1"
  },
  "devDependencies": {
    "chai": "4.1.1",
    "codeclimate-test-reporter": "0.5.0",
    "babel-eslint": "^8.0.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-node": "^5.2.1",
    "husky": "0.14.3",
    "nyc": "1.1.0-alpha.1"
  }
}
